{		
 	"configVersion": "1",		
 	"deviceDetected": false,		
 	"https": false,		
 	"liveSite": false,		
 	"locales": ["en"],		
 	"templateVersion": 3,		
 	"features": [		
 		{		
 			"name": "Geocode",		
 			"geocodeIP": {		
 				"urlTemplate": "desktop.urls.geocodeip"		
 			}		
		},
		{
			"name": "Locations",
			"locationPageSet": {
				"urlTemplate": "desktop.urls.locationPageSet1",
				"template": "desktop.location.default",
				"outputFormat": ["Html", "Json"],
				"locationScope" : {
					"savedSearch" : "ny"
				}
			}
		},
		{
			"name": "Locations 2",
			"locationPageSet": {
				"urlTemplate": "desktop.urls.locationPageSet2",
				"template": "desktop.location.default",
				"outputFormat": ["Html", "Json"],
				"locationScope" : {
					"savedSearch" : "ak1"
				}
			}
		},
		{
			"name": "Location Directory 1",
			"locationDirectory": {
				"levels": [
					{
						"urlTemplate": "desktop.urls.directory1State",
						"template": "desktop.directory.stateList",
						"field": "State",
						"orderBy": {
							"field": "State",
							"order": "ascending"
						}
					},
					{
						"urlTemplate": "desktop.urls.directory1Location",
						"template": "desktop.directory.locationList",
						"field": "Locations",
						"orderBy": {
							"field": "Locations",
							"order": "ascending"
						}
					}
				],
				"locationPageSet": "Locations",
				"outputFormat": ["Html", "Json"]
			}
		},
		{
			"name": "Location Directory 2",
			"locationDirectory": {
				"levels": [
					{
						"urlTemplate": "desktop.urls.directory2State",
						"template": "desktop.directory.stateList",
						"field": "State",
						"orderBy": {
							"field": "State",
							"order": "ascending"
						}
					},
					{
						"urlTemplate": "desktop.urls.directory2Location",
						"template": "desktop.directory.locationList",
						"field": "Locations",
						"orderBy": {
							"field": "Locations",
							"order": "ascending"
						}
					}
				],
				"locationPageSet": "Locations 2",
				"outputFormat": ["Html", "Json"]
			}
		}
 	]		
 }
